            "textColor": "#4B5446",
            "font": "t-heading-3",
            "blockClass": "tres-porteiras-prods-title"

        }
    },
    "search-result-layout.customQuery#tres-porteiras": {
        "blocks": [
            "search-result-layout.desktop#tres-porteiras",
            "search-result-layout.mobile",
            "search-not-found-layout"
        ],
        "props": {
            "querySchema": {
            "skusFilter": "ALL_AVAILABLE",
            "queryField": "Caf%C3%A9%20tres-porteiras",
            "mapField": "b",
            "simulationBehavior": "default",
            "maxItemsPerPage": 15,
            "orderByField": "OrderByTopSaleDESC",
            "hideUnavailableItems": true
            }        
        }
    },
    "search-result-layout.desktop#tres-porteiras": {
        "children": [
            "flex-layout.row#result-tres-porteiras"
        ],
        "props": {
            "pagination": "show-more",
            "preventRouteChange": false,
            "mobileLayout": {
            "mode1": "small",
            "mode2": "normal"
            }
        }
    },
    "flex-layout.row#result-tres-porteiras": {
        "children": ["flex-layout.col#content-tres-porteiras"],
        "props": {
            "preventHorizontalStretch": true,
            "fullWidth": true
        }
    },
    "flex-layout.col#content-tres-porteiras": {
        "children": [
            "flex-layout.row#fetchprevious",
            "flex-layout.row#products",
            "flex-layout.row#fetchmore"
        ],
        "props": {
            "width": "grow",
            "preventVerticalStretch": true
        }
    }
}